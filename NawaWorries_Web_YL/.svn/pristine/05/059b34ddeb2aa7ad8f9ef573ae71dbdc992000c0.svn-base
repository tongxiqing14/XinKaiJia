public class EnemyTeam {

    private String enemyName = "";
    private int type = 0;
    private double hpNumber = 0;  //core number
    private double fight_number = 0;    //core number
    private boolean dead = false;

    public EnemyTeam() {
    }

    public EnemyTeam(String enemyName, int type, double hpNumber, double fight_number, boolean dead) {
        this.enemyName = enemyName;
        this.type = type;
        this.hpNumber = hpNumber;
        this.fight_number = fight_number;
        this.dead = dead;
    }

    public String getEnemyName() {
        return enemyName;
    }

    public void setEnemyName(String enemyName) {
        this.enemyName = enemyName;
    }

    public double getHpNumber() {
        return hpNumber;
    }

    public void setHpNumber(int hpNumber) {
        this.hpNumber = hpNumber;
    }

    public int getType() {
        return type;
    }

    public void setType(int type) {
        this.type = type;
    }

    public boolean isDead() {
        return dead;
    }

    public void setDead(boolean dead) {
        this.dead = dead;
    }

    public double getFight_number() {
        return fight_number;
    }

    public void setFight_number(int fight_number) {
        this.fight_number = fight_number;
    }

    @Override
    public String toString() {
        return "EnemyTeam{" +
                "enemyName='" + enemyName + '\'' +
                ", type=" + type +
                ", hpNumber=" + hpNumber +
                ", fight_number=" + fight_number +
                ", dead=" + dead +
                '}';
    }
}
