function enemy_effect_group_stage_1(){

    for(var i = 0 ; i < 5; i++){
        if(document.enemy_anger_x[i] < 95){
            switch (i){
                case 0:
                    document.enemy_anger_x[i] ++;      // freeze time
                    break;
                case 1:
                    document.enemy_anger_x[i] +=2;      // freeze time
                    break;
                case 2:
                    document.enemy_anger_x[i] +=2;      // freeze time
                    break;
                case 3:
                    document.enemy_anger_x[i] +=4;      // freeze time
                    break;
                case 4:
                    document.enemy_anger_x[i] +=4;      // freeze time
                    break;
            }
        }else{
            switch (i){
                case 0:
                    document.enemy_anger_x[i] = 0;
                    watership_right_effect("img_right_enemy_dazhao1_id","div_right_enemy_dazhao1_id");
                    break;
                case 1:
                    document.enemy_anger_x[i] = 0;
                    right_falling_fire_effect();
                    break;
                case 2:
                    document.enemy_anger_x[i] = 0;
                    right_thorns_effect();
                    break;
                case 3:
                    document.enemy_anger_x[i] = 0;
                    right_barrage_effect();
                    break;
                case 4:
                    document.enemy_anger_x[i] = 0;
                    right_fire_effect();
                    break;
            }

        }
    }
    setTimeout("enemy_effect_group_stage_1()",700);         // control the FPS

}

